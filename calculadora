import math
from math import *
op = input("Type of operation: ")
if op == "!":
    n1 = int(input("First number: "))
    r = factorial(n1)
elif op == "sqrt":
    n1 = float(input("First number: "))
    r = sqrt(n1)
else:
    n2 = float(input("Second number: "))
    if op == "+":
        r = n1 + n2
    elif op == "-":
        r = n1 - n2
    elif op == "/":
        r = n1 / n2
    elif op == "*":
        r = n1 * n2
    elif op == "^":
        r = pow(n1, n2)
print(r)
